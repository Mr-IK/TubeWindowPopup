{
  "name": "TubeWindowPopup",
  "version": "1.2",
  "description": "Youtube動画をポップアップ表示するやつ",
  "browser_action": {
    "default_popup": "popup/popup.html"
  },
  "permissions": ["tabs"],
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_security_policy": "script-src 'self' https://code.getmdl.io/; object-src 'self'",
  "manifest_version": 2
}
